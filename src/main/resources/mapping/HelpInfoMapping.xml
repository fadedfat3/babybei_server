<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zhumingbei.babybei_server.mapper.HelpInfoMapper">
    <select id="findByType" resultType="com.zhumingbei.babybei_server.bean.HelpInfo">
        select * from help_info where `type` = #{type} limit #{offset}, #{limit};
    </select>
</mapper>